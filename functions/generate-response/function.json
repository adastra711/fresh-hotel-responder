{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "generate-response"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/generate-response/index.js",
  "cors": {
    "allowedOrigins": [
      "*"
    ],
    "supportCredentials": true
  }
} 